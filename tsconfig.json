{
  "exclude": [
    "./node_modules"
  ],
  "compilerOptions": {
    "target": "ESNext",
    "moduleResolution": "Node",
    "module": "CommonJS",
    //"declaration": true,
    //"sourceMap": true,
    "outDir": "./lib",
    "rootDir": "./src",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "allowSyntheticDefaultImports": true
  }
}
